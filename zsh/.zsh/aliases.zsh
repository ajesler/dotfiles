alias v=nvim

# Navigation
alias dev="cd ~/dev"
alias dotfiles="cd ~/dotfiles"

# System
alias ls="ls -alhG"

# Shell Config
alias zshr="source ~/.zshrc"

# Git
alias g=git
alias gap="git add -p"
alias gcv="git commit -v"
alias gco="git checkout"
alias gs="git status"
alias gd="git diff"
alias gds="git diff --cached"
alias gcl="git clone"

# Tmuxinator
alias tm=tmuxinator
